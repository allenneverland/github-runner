# GitHub Runner 設定
ACCESS_TOKEN=ghp_your_github_token_here

# 可選：使用預建映像而不是本地建置 (取消註解下面這行)
# USE_PREBUILT_IMAGE=true
# DOCKER_IMAGE=your-dockerhub-username/github-runner-tools:latest

# Runner 性能調優
RUST_CARGO_INCREMENTAL=1
RUST_CARGO_NET_RETRY=10
NODE_OPTIONS=--max-old-space-size=2048

# 快取設定
CARGO_NET_OFFLINE=false
NPM_CONFIG_AUDIT=false
PIP_CACHE_DIR=/home/runner/.cache/pip

# 資源限制 (可選)
RUNNER_MEMORY_LIMIT=4g
RUNNER_CPU_LIMIT=2.0